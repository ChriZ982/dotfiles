{
	"name": "dotfiles",
	"runArgs": [
		"--name",
		"dev_dotfiles"
	],
	"build": {
		"dockerfile": "Dockerfile"
	},
	"mounts": [
		"source=dev_dotfiles_home,target=/home/ubuntu,type=volume"
	],
	"customizations": {
		"vscode": {
			"extensions": [
				"timonwong.shellcheck",
				"bungcip.better-toml",
				"skyapps.fish-vscode"
			]
		}
	}
}
